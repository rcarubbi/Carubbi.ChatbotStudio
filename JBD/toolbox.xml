          <field name="Language">pt-BR</field>
          <field name="SubscriptionKey"></field>
          <field name="Endpoint"></field>
        </shadow>
      </value>
    </block>
    <block type="formstep" collapsed="true">
      <field name="Id"></field>
      <field name="Durable">FALSE</field>
